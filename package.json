{
  "name": "nextron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "electron-packager ./ --plataform-PLATAFORM --arch_xARCH --out ./DESTINATION-DIRECTORY --app-version VERSION-YOUR-APP --overwrite",
    "start": "concurrently \"cross-env BROWSER=none next dev\" \"wait-on http://localhost:3000 && electron .\""
  },
  "dependencies": {
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.1",
    "cross-env": "^7.0.3",
    "electron": "^12.0.2",
    "wait-on": "^5.3.0"
  }
}
